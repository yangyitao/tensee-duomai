<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0" />
    <meta name="format-detection" content="telephone=no" />
    <title>区域员工排名</title>
    <link href="../../../css/aui.css" rel="stylesheet" />
    <style>
        .aui-list-view-cell {
            padding: 5px;
        }

        .form {
            width: 100%;
            text-align: center;
            font-size: 13px;
            border-collapse: collapse;
        }

            .form td, .form th {
                border-bottom: 1px solid rgba(216, 216, 216, 0.1);
                height: 35px;
                letter-spacing: 0.4px;
            }

            .form th {
                font-weight: 400;
            }

            .form tbody td {
                color: #8f8f94;
            }

                .form tbody td:first-child {
                    color: #000;
                }
    </style>
</head>
<body>
    <div class="aui-content" id="divTitle">
        <ul class="aui-list-view" style="margin-bottom: 0px;">
            <li class="aui-list-view-cell" style="padding: 3px;">
                <table class="form">
                    <thead>
                        <tr>
                            <td style="width: 25%;">零售部</td>
                            <td style="width: 25%;">区域</td>
                            <td style="width: 25%;">姓名</td>
                            <td style="width: 15%;">达成</td>
                            <td style="width: 10%;">排名</td>
                        </tr>
                    </thead>
                </table>
            </li>
        </ul>
    </div>
</body>
</html>

<script src="../../../script/zepto.js"></script>
<script src="../../../script/api.js"></script>
<script src="../../../script/honesty.base.js"></script>
<script type="text/javascript">
    apiready = function () {
        api.parseTapmode();
        $(function () {
            api.openFrame({
                name: "AreaEmpRank_AreaEmpRank_AreaEmpRankInfo_FRM",
                url: "AreaEmpRankInfo.html",
                pageParam: {
                    OrganizeID: api.pageParam.OrganizeID,
                    Year: api.pageParam.Year,
                    Month: api.pageParam.Month,
                    LevelDesc: api.pageParam.LevelDesc
                },
                rect: {
                    x: 0,
                    y: $("#divTitle").height() + $honesty.RangeTop(45),
                    w: $("#divTitle").width(),
                    h: 'auto'
                }
            })
        });

    }
</script>
